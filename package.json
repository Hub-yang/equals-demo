{
  "name": "js-equals-demo",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.3",
  "description": "It's a two-dimensional chart demo to describe equality comparisons of common data types in JavaScript",
  "author": "HuberyYang <mochenghualeishuai@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alfred-Skyblue/vue-draggable-plus"
  },
  "keywords": [
    "js",
    "javascript",
    "equals",
    "equality",
    "equality table"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "dependencies": {
    "@octokit/core": "^6.0.1",
    "@vueuse/core": "^10.7.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/types": "^19.0.3",
    "@iconify-json/carbon": "^1.1.28",
    "@iconify-json/ic": "^1.1.17",
    "@iconify-json/mdi": "^1.1.64",
    "@types/node": "^20.11.5",
    "@unocss/eslint-config": "^0.58.3",
    "@unocss/eslint-plugin": "^0.58.3",
    "@unocss/reset": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue-macros/volar": "^0.18.11",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.14.3",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.10",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit $1"
    }
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
